<h2>Lista de Actos Inscribibles (trámites)</h2>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <table class="table table-hover" style="width:100%">
      <tr>
        <th>Seleccionar Acto Inscribible</th>
        <th>Acción</th>
      </tr>
      <% Asiento::TIPO_TRAMITE.each do |t| %>
        <tr>
          <td><%= t.label %></td> 
          <td><%= link_to "Crear", t.value %></td> 
        </tr>
      <% end %>
    </table>
  </div>
</div>
