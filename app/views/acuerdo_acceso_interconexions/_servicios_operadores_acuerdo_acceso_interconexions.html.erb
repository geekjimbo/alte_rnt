<div class="panel panel-default">
  <div class="panel-heading">Servicios del Operador</div>
    <div class="panel-body">
      <div class="row">
        <div class="col-md-2">
          <div class="form-group">
              <%=  f.hidden_field :id %>
              <%=  f.label :seleccionar_servicio %>
              <%=  f.collection_select(:sci_servicios_id, @servicios, :id, :descripcion) unless @servicios.nil?%>
          </div>
        </div>

        <div class="col-md-3">
          <div class="form-group">
            <%=  f.label "Precio" %>
            <%=  f.number_field :precio_interconexion, class: 'form-control', :step => 0.01 %>
          </div>
        </div>
      </div>

    </div>
</div>
